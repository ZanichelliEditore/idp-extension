{
    "name": "zanichelli/idp-extensions",
    "description": "Classes to interact with the Zanichelli identity provider",
    "authors": [
        {
            "name": "Zanichelli",
            "email": "developers@zanichelli.it"
        }
    ],
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.0",
        "orchestra/testbench": "^3.8"
    },
    "minimum-stability": "dev",
    "require": {
        "laravel/framework": "^6|^7|^8|^9|^10",
        "guzzlehttp/guzzle": "~7.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zanichelli\\IdpExtension\\Providers\\IdpProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Zanichelli\\IdpExtension\\": "src/"
        }
    },
    "license": "MIT"
}
