{
  "name": "zanichelli/idp-extensions",
  "description": "Classes to interact with the Zanichelli identity provider",
  "authors": [
    {
      "name": "Zanichelli",
      "email": "developers@zanichelli.it"
    }
  ],
  "require-dev": {
    "mockery/mockery": "^1.0",
    "orchestra/testbench": "^3.8"
  },
  "minimum-stability": "dev",
  "require": {
    "laravel/framework": "^8|^9|^10|^11|^12",
    "guzzlehttp/guzzle": "~7.0",
    "firebase/php-jwt": "^6.10"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Zanichelli\\IdpExtension\\Providers\\IdpProvider"
      ]
    }
  },
  "autoload": {
    "psr-4": {
      "Zanichelli\\IdpExtension\\": "src/"
    }
  },
  "license": "MIT"
}
